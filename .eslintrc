{
    "extends": [
        "plugin:prettier/recommended",
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "env": {
        "node": true,
        "browser": true
    },
    "globals": {
        "route": true,
        "$route": true
    }
}
